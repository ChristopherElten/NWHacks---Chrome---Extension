{
  "manifest_version": 2,
  "name": "NW Hacks!",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.2.1.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "downloads"
  ],

  "browser_action": {
  "default_icon": "icon.png"
  }
}

